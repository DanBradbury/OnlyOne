            <kind>1</kind>
            <string>draw_set_font(menu_title); draw_set_color(c_red);

draw_text(0,0, "Back");

i = ds_list_size(world1);
hor = 0; vert = 0;
for(j=0;j&lt;i;j++){
    v = ds_list_find_value(world1, j);
    
    
    if(ds_map_find_value(v,"completed")==true){
        draw_sprite(spr_levelCircle, 0, 100+(j*100), 100);
        if(ds_map_find_value(v,"direction")==RIGHT){
            draw_sprite(spr_horizontal, 0, 100+55, 100+10.5);    
        }else{
            draw_sprite(spr_vertical, 0, 100, 100); 
        }
        
    }else{
       
       deltaX = (j*100);
       if(j!=0){
        deltaX += 55;
       }
       deltaX -= vert*100;
       
       deltaY = vert*100;
        
        if(ds_map_find_value(v, "levelType")==BOSS){
            draw_sprite(spr_boss, 0, 100+deltaX, 100+deltaY+55);
        }else{
            if(ds_map_find_value(v, "levelType")==PLATFORM){
                draw_sprite(spr_levelCircle, 0, 100+deltaX, 100);
            }else if(ds_map_find_value(v, "levelType") == RUNNER){
                draw_sprite(spr_levelCircle, 1, 100+deltaX, 100);
            }
        }
        if(ds_map_find_value(v,"direction")==RIGHT){
            draw_sprite(spr_horizontal, 1, 100+deltaX+55, 100);    
        }else{
            draw_sprite(spr_vertical, 1, 100+deltaX, 100+42); 
        }
    }
    if( ds_map_find_value(v, "directon")==RIGHT){
        hor += 1;   
    }else if(ds_map_find_value(v, "direction") ==DOWN){
        vert += 1;
    }  
    
}

</string>
